  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#908ca4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#a8d4a8"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape", "string.regexp"],
      "settings": {
        "foreground": "#e0a8c0"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e8a888"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#8ce0a0"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#e8a888"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.new",
        "keyword.operator.expression",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f0a0c8"
      }
    },
    {
      "name": "Control Flow",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#e879a8"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#f0a0c8"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#c4b5f0"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#88c4f0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class/Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#7dd3d0"
      }
    },
    {
      "name": "Type Annotation",
      "scope": [
        "meta.type.annotation",
        "entity.name.type.alias",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#7dd3d0"
      }
    },
    {
      "name": "Type Parameter",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#5ccfb8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e8e6f0"
      }
    },
    {
      "name": "Variable Property",
      "scope": ["variable.other.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Variable Builtin",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#88c4f0"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#e0a8c0"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#c4c0d4"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#f0a0c8"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#c4b5f0"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#e8a888"
      }
    },
    {
      "name": "CSS Unit",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#e8a888"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#7dd3d0"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#c4b5f0"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#c4b5f0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#c4b5f0",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a8d4a8"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#c4c0d4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List",
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#f0a0c8"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#8ce0a0"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#e88090"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#e8d088"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e88090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#908ca4",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "class": "#7dd3d0",
    "interface": "#7dd3d0",
    "type": "#7dd3d0",
    "typeParameter": "#5ccfb8",
    "enum": "#7dd3d0",
    "enumMember": "#e8a888",
    "namespace": "#7dd3d0",
    "function": "#c4b5f0",
    "method": "#c4b5f0",
    "macro": "#e0a8c0",
    "variable": "#e8e6f0",
    "variable.readonly": "#e8e6f0",
    "variable.local": "#e8e6f0",
    "variable.local.readonly": "#e8e6f0",
    "parameter": "#88c4f0",
    "property": "#a78bfa",
    "property.readonly": "#a78bfa",
    "string": "#a8d4a8",
    "number": "#e8a888",
    "keyword": "#f0a0c8",
    "operator": "#f0a0c8",
    "comment": { "foreground": "#908ca4", "fontStyle": "italic" },
    "decorator": "#a78bfa",
    "*.deprecated": { "fontStyle": "strikethrough" }
  }
}

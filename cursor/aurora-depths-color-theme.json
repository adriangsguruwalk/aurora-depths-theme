  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8a95a6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#a3c9a8"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape", "string.regexp"],
      "settings": {
        "foreground": "#c9a0b8"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e0a88c"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#8fcf9a"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#e0a88c"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.new",
        "keyword.operator.expression",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#7dd3c4"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#b4a7d6"
      }
    },
    {
      "name": "Class/Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#d4a5c9"
      }
    },
    {
      "name": "Type Annotation",
      "scope": [
        "meta.type.annotation",
        "entity.name.type.alias",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#d4a5c9"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e5eaf2"
      }
    },
    {
      "name": "Variable Property",
      "scope": ["variable.other.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#e5eaf2"
      }
    },
    {
      "name": "Variable Builtin",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#b4a7d6"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#c9a0b8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#b8c5d4"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b4a7d6"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#e0a88c"
      }
    },
    {
      "name": "CSS Unit",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#e0a88c"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#d4a5c9"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#91c4d8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#91c4d8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a3c9a8"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#b8c5d4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List",
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#91c4d8"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#8fcf9a"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#d4878f"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#e6c783"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d4878f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#8a95a6",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "class": "#d4a5c9",
    "interface": "#d4a5c9",
    "type": "#d4a5c9",
    "typeParameter": "#d4a5c9",
    "enum": "#d4a5c9",
    "enumMember": "#e0a88c",
    "namespace": "#d4a5c9",
    "function": "#7dd3c4",
    "method": "#7dd3c4",
    "macro": "#c9a0b8",
    "variable": "#e5eaf2",
    "variable.readonly": "#e5eaf2",
    "variable.local": "#e5eaf2",
    "variable.local.readonly": "#e5eaf2",
    "parameter": "#e5eaf2",
    "property": "#e5eaf2",
    "property.readonly": "#e5eaf2",
    "string": "#a3c9a8",
    "number": "#e0a88c",
    "keyword": "#91c4d8",
    "operator": "#91c4d8",
    "comment": { "foreground": "#8a95a6", "fontStyle": "italic" },
    "decorator": "#b4a7d6",
    "*.deprecated": { "fontStyle": "strikethrough" }
  }
}
